<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:fitsSystemWindows="true"
    android:clipToPadding="true"
    android:orientation="vertical"
    android:background="@color/theme_bg_color"
    android:layout_height="match_parent">
    <include layout="@layout/title_layout"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_marginTop="@dimen/padding_16"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_starttv"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加起点"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/padding_8"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_throughp1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加途径点"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/padding_8"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_throughp2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加途径点"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/padding_8"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_throughp3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加途径点"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/padding_8"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_throughp4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加途径点"/>
            </LinearLayout>
            <LinearLayout
                android:layout_marginTop="@dimen/padding_8"
                android:background="@color/color_ffffff"
                android:gravity="center_vertical"
                android:paddingLeft="@dimen/padding_16"
                android:paddingRight="@dimen/padding_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/button_height52">
                <TextView
                    android:id="@+id/settting_throughp5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:textSize="@dimen/dp_15"
                    android:textColor="@color/color_000000"
                    android:text="请添加途径点"/>
            </LinearLayout>
            <TextView
                android:id="@+id/commit_navibt"
                android:layout_marginRight="@dimen/padding_16"
                android:layout_marginLeft="@dimen/padding_16"
                android:background="@drawable/button_bg"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_48"
                android:text="马上出发"
                android:textSize="@dimen/dp_17"
                android:textColor="@color/color_ffffff"
                android:gravity="center"
                android:layout_marginTop="@dimen/dp_30"/>
        </LinearLayout>
    </ScrollView>

</LinearLayout>